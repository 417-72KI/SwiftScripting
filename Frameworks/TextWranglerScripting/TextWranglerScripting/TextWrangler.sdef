  <suite name="Text Suite" code="R*ch" description="Classes and types for scripting text manipulations">
    <class name="text_object" code="txtO" description="abstract class describing a text object and its basic properties" inherits="item">
      <property name="contents" code="pcnt" type="any" description="literal text contents"/>
      <property name="length" code="leng" type="integer" access="r" description="length of text object (in characters)"/>
      <property name="characterOffset" code="Ofse" type="integer" access="r" description="offset of a text object from the beginning of the document (first char has offset 1)"/>
      <property name="startLine" code="SLin" type="integer" access="r" description="line number of the first “hard” line containing this text object (first line has number 1)"/>
      <property name="endLine" code="ELin" type="integer" access="r" description="line number of the last “hard” line containing this text object"/>
      <property name="startColumn" code="SCol" type="integer" access="r" description="character column (1 is first) of the first character of the object"/>
      <property name="endColumn" code="ECol" type="integer" access="r" description="character column (1 is first) of the last character of the object"/>
      <property name="startDisplayLine" code="SDLn" type="integer" access="r" description="line number of the first display line containing this text object (first line has number 1)"/>
      <property name="endDisplayLine" code="EDLn" type="integer" access="r" description="line number of the last display line containing this text object"/>
    </class>
    <class name="insertion point" code="cins" description="an insertion point" inherits="text_object"/>
