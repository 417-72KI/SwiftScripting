  <suite name="Unix Scripting" code="¶erl" description="events supported by the Unix scripting tools">
    <command name="run unix script" code="¶erlRSpt" description="runs the unix script in the front BBEdit window">
      <direct-parameter optional="yes" description="list of values to be passed to @ARGV">
        <type type="any" list="yes"/>
      </direct-parameter>
      <parameter name="selection only" code="¶son" type="boolean" optional="yes" description="execute the selection as a unix script"/>
      <parameter name="save before running" code="¶sbr" type="boolean" optional="yes" description="save the front window before running"/>
      <parameter name="use debugger" code="¶dbg" type="boolean" optional="yes" description="use the perl debugger"/>
      <parameter name="run in terminal" code="¶bpf" type="boolean" optional="yes" description="run the script in the Terminal"/>
      <parameter name="chdir to script folder" code="¶chd" type="boolean" optional="yes" description="set the working directory to the script's parent folder"/>
      <parameter name="output to" code="¶pou" type="¶OMd" optional="yes" description="the output mode to use"/>
      <parameter name="clear before writing" code="¶cbw" type="boolean" optional="yes" description="clear the output window before writing"/>
      <parameter name="save after writing" code="¶saw" type="boolean" optional="yes" description="save the output window after writing"/>
    </command>
    <command name="run unix filter" code="¶erlRFtr" description="runs the specified unix filter on the selection in the frontmost BBEdit window">
      <direct-parameter type="file" description="the filter to run"/>
      <parameter name="output STDERR to" code="¶pou" type="¶FOM" optional="yes" description="the output mode to use for STDERR"/>
      <parameter name="clear before writing" code="¶cbw" type="boolean" optional="yes" description="clear the output window before writing"/>
      <parameter name="save after writing" code="¶saw" type="boolean" optional="yes" description="save the output window after writing"/>
      <parameter name="replacing selection" code="o2sl" type="boolean" optional="yes" description="does the filter output replace the selection?"/>
    </command>
    <enumeration name="¶OMd" code="¶OMd">
      <enumerator name="Unix script output window" code="mprl"/>
      <enumerator name="new untitled window" code="nwin"/>
      <enumerator name="script log output window" code="lwin"/>
    </enumeration>
    <enumeration name="¶FOM" code="¶FOM">
      <enumerator name="Unix script output window" code="mprl"/>
