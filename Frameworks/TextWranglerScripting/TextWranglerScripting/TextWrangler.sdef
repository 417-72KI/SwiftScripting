<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionary SYSTEM "file://localhost/System/Library/DTDs/sdef.dtd">
<dictionary>
  <suite name="Required Suite" code="reqd" description="Terms that every application should support">
    <command name="open" code="aevtodoc" description="Opens the specified item(s)">
      <direct-parameter type="specifier" description="the item(s) to open"/>
      <parameter name="with properties" code="prdt" type="record" optional="yes" description="properties to be applied to the document when opening"/>
      <parameter name="opening in" code="OpnI" type="any" optional="yes" description="existing text editing window to open the document in, or one of front_window/new_window/separate_windows"/>
      <parameter name="stationery awareness" code="SAwr" type="boolean" optional="yes" description="honor the “Stationery” flag for documents?"/>
      <parameter name="LF translation" code="LFtx" type="boolean" optional="yes" description="convert line feeds in open documents? (By Default, the preference setting.)"/>
      <parameter name="read only" code="RdOn" type="boolean" optional="yes" description="open documents read-only? (By Default, false.)"/>
      <parameter name="reading as" code="RdAs" type="RAsE" optional="yes" description="interpret the contents of the file in which way (By Default, auto-detect file type)"/>
      <parameter name="adding to recent list" code="Rcnt" type="boolean" optional="yes" description="add to the application's recent file list? (By Default, true.)"/>
    </command>
    <command name="print" code="aevtpdoc" description="Print the specified object(s)">
      <direct-parameter type="specifier" description="the item(s) to print"/>
      <parameter name="with properties" code="prdt" type="print settings" optional="yes" description="the print settings"/>
      <parameter name="print dialog" code="pdlg" type="boolean" optional="yes" description="Should the application show the print dialog?"/>
    </command>
    <command name="run" code="aevtoapp" description="Sent to an application when it is double-clicked"/>
    <command name="quit" code="aevtquit" description="Quit application"/>
    <class name="print settings" code="pset">
      <property name="copies" code="lwcp" type="integer" access="r" description="the number of copies of a document to be printed "/>
      <property name="collating" code="lwcl" type="boolean" access="r" description="Should printed copies be collated?"/>
      <property name="starting page" code="lwfp" type="integer" access="r" description="the first page of the document to be printed"/>
      <property name="ending page" code="lwlp" type="integer" access="r" description="the last page of the document to be printed"/>
      <property name="pages across" code="lwla" type="integer" access="r" description="number of logical pages laid across a physical page"/>
      <property name="pages down" code="lwld" type="integer" access="r" description="number of logical pages laid out down a physical page"/>
      <property name="requested print time" code="lwqt" type="date" access="r" description="the time at which the desktop printer should print the document..."/>
      <property name="error handling" code="lweh" type="enum" access="r" description="how errors are handled"/>
      <property name="printer features" code="lwpf" access="r" description="for printer specific features">
        <type type="list" list="yes"/>
      </property>
      <property name="fax number" code="faxn" type="text" access="r" description="for fax number"/>
      <property name="target printer" code="trpr" type="text" access="r" description="for target printer"/>
      <property name="printing font" code="PFNm" type="text" access="r"/>
      <property name="printing font size" code="PFSz" type="integer" access="r"/>
      <property name="printing font style" code="PFSt" type="text" access="r"/>
      <property name="printing tab width" code="PTbW" type="integer" access="r"/>
      <property name="using document font" code="PDcF" type="boolean" access="r"/>
      <property name="framing page" code="Pfrm" type="boolean" access="r"/>
      <property name="page headers" code="Phdr" type="boolean" access="r"/>
      <property name="line numbers" code="Pln#" type="boolean" access="r"/>
      <property name="gutter" code="Pgut" type="boolean" access="r"/>
      <property name="full path" code="Ppth" type="boolean" access="r"/>
      <property name="print color syntax" code="Pclr" type="boolean" access="r"/>
      <property name="time stamp" code="Pstp" type="Pstp" access="r"/>
    </class>
    <enumeration name="enum" code="enum">
      <enumerator name="standard" code="lwst" description="Standard PostScript error handling  "/>
      <enumerator name="detailed" code="lwdt" description="print a detailed report of PostScript errors"/>
    </enumeration>
    <enumeration name="RAsE" code="RAsE">
      <enumerator name="auto_detect file" code="RdAD"/>
      <enumerator name="UTF8 file" code="RdU8"/>
      <enumerator name="Unicode File" code="RdUb"/>
      <enumerator name="Byte Swapped Unicode File" code="RdUl"/>
    </enumeration>
    <enumeration name="Pstp" code="Pstp">
      <enumerator name="modification date" code="asmo"/>
      <enumerator name="printing date" code="PrnD"/>
    </enumeration>
    <enumeration name="OpnE" code="OpnE">
      <enumerator name="front_window" code="FrtW"/>
      <enumerator name="new_window" code="NewW"/>
      <enumerator name="separate_windows" code="SepW"/>
    </enumeration>
  </suite>
  <suite name="Standard Suite" code="****" description="Common terms for most applications">
    <command name="close" code="coreclos" description="Closes the designated window.">
      <direct-parameter type="specifier" description="The window to close."/>
      <parameter name="saving" code="savo" type="savo" optional="yes" description="Save the file before closing? (Defaults to “ask”.)"/>
      <parameter name="saving in" code="kfil" type="alias" optional="yes" description="If saving, this parameter specifies the file in which the closed window is to be saved. (By default, the same file it was read from.)"/>
    </command>
    <command name="count" code="corecnte" description="Return the number of elements of a particular class within an object">
      <direct-parameter type="specifier" description="the object whose elements are to be counted"/>
      <parameter name="each" code="kocl" type="type" description="the class of the elements to be counted. Keyword 'each' is optional in AppleScript"/>
      <result type="integer" description="the number of elements"/>
    </command>
    <command name="delete" code="coredelo" description="Delete an element from an object">
      <direct-parameter type="specifier" description="the element to delete"/>
    </command>
    <command name="duplicate" code="coreclon" description="Duplicate object(s)">
      <direct-parameter type="specifier" description="the object(s) to duplicate"/>
      <parameter name="to" code="insh" type="location specifier" optional="yes" description="the new location for the object(s)"/>
      <result type="specifier" description="to the duplicated object(s)"/>
    </command>
    <command name="exists" code="coredoex" description="Verify if an object exists">
      <direct-parameter type="specifier" description="the object in question"/>
      <result type="boolean" description="true if it exists, false if not"/>
    </command>
    <command name="move" code="coremove" description="Move object(s) to a new location">
      <direct-parameter type="specifier" description="the object(s) to move"/>
      <parameter name="to" code="insh" type="location specifier" description="the new location for the object(s)"/>
      <result type="specifier" description="to the object(s) after they have been moved"/>
    </command>
    <command name="get" code="coregetd">
      <direct-parameter type="specifier"/>
      <parameter name="as" code="rtyp" optional="yes">
        <type type="type" list="yes"/>
      </parameter>
      <result type="any"/>
    </command>
    <command name="make" code="corecrel" description="Make a new element">
      <parameter name="new" code="kocl" type="type" description="the class of the new element. Keyword 'new' is optional in AppleScript"/>
      <parameter name="at" code="insh" type="location specifier" optional="yes" description="the location at which to insert the element"/>
      <parameter name="with properties" code="prdt" type="record" optional="yes" description="the initial values for the properties of the element"/>
      <parameter name="with data" code="data" type="any" optional="yes" description="the initial data for the element"/>
      <parameter name="to" code="to  " type="specifier" optional="yes" description="when creating an alias, the original item to create an alias to"/>
      <parameter name="initial save location" code="kfil" type="file" optional="yes" description="when creating a document, the initial location to create the document's disk file"/>
      <result type="specifier" description="to the new object(s)"/>
    </command>
    <command name="save" code="coresave" description="Save a window or document.">
      <direct-parameter type="specifier" description="the object to save"/>
      <parameter name="to" code="kfil" type="alias" optional="yes" description="file in which to save the window’s contents"/>
      <parameter name="saving as stationery" code="SASt" type="boolean" optional="yes" description="save the file as stationery? (By Default, false.)"/>
      <parameter name="add to recent list" code="Rcnt" type="boolean" optional="yes" description="add to the application's recent file list? (By Default, true.)"/>
    </command>
    <command name="set" code="coresetd">
      <direct-parameter type="specifier"/>
      <parameter name="to" code="data" type="any"/>
    </command>
    <enumeration name="savo" code="savo">
      <enumerator name="yes" code="yes " description="Save objects now"/>
      <enumerator name="no" code="no  " description="Do not save objects"/>
      <enumerator name="ask" code="ask " description="Ask the user whether to save"/>
    </enumeration>
    <enumeration name="kfrm" code="kfrm">
      <enumerator name="index" code="indx" description="keyform designating indexed access"/>
      <enumerator name="named" code="name" description="keyform designating named access"/>
      <enumerator name="id" code="ID  " description="keyform designating identifier access"/>
    </enumeration>
  </suite>
  <suite name="Miscellaneous" code="misc" description="Miscellaneous useful events.">
    <command name="activate" code="miscactv" description="activate the application or a window">
      <direct-parameter type="specifier" optional="yes" description="reference to the object to activate"/>
    </command>
    <command name="copy" code="misccopy" description="Copy an object to the clipboard">
      <direct-parameter type="specifier" optional="yes" description="the object to copy to the clipboard"/>
      <parameter name="appending to clipboard" code="Apnd" type="boolean" optional="yes" description="append to the current clipboard? (By Default, false.) If you are going to use this from AppleScript, you'll have to use “extended copy” because it will prevent you from compiling this form."/>
    </command>
    <command name="extended copy" code="R*chECpy" description="Copy an object to the clipboard - supplied as an alternative to AppleScript users since it refuses to compile “copy with append to clipboard”">
      <direct-parameter type="specifier" optional="yes" description="the object to copy to the clipboard"/>
      <parameter name="appending to clipboard" code="Apnd" type="boolean" optional="yes" description="append to the current clipboard? (By Default, false.)"/>
    </command>
    <command name="cut" code="misccut " description="Cut an object to the clipboard">
      <direct-parameter type="specifier" optional="yes" description="the object to cut to the clipboard"/>
      <parameter name="appending to clipboard" code="Apnd" type="boolean" optional="yes" description="append to the current clipboard? (By Default, false.)"/>
    </command>
    <command name="paste" code="miscpast" description="Paste an object from the clipboard">
      <direct-parameter type="specifier" optional="yes"/>
    </command>
    <command name="paste column" code="miscPstC" description="Paste an text from the clipboard forcing insertion as a rectangular column">
      <direct-parameter type="specifier" optional="yes"/>
    </command>
    <command name="redo" code="miscredo" description="Reverses the effects of the most recent Undo, if possible.">
      <direct-parameter type="specifier" optional="yes"/>
    </command>
    <command name="revert" code="miscrvrt" description="Restore the object to its last saved state">
      <direct-parameter type="specifier" description="the object to revert"/>
      <parameter name="discarding unsaved changes" code="revo" type="revo" optional="yes" description="Discard unsaved changes? (Defaults to yes.)"/>
    </command>
    <command name="select" code="miscslct" description="Select the specified object">
      <direct-parameter type="specifier" optional="yes" description="the object to select"/>
    </command>
    <command name="undo" code="miscundo" description="Undoes the effects of the last command, if possible.">
      <direct-parameter type="specifier" optional="yes"/>
    </command>
    <enumeration name="revo" code="revo">
      <enumerator name="yes" code="yes " description="discard changes now"/>
      <enumerator name="ask" code="ask " description="ask the user whether to discard changes"/>
    </enumeration>
  </suite>
  <suite name="BBEdit Suite" code="R*ch" description="Terms specific to BBEdit">
    <command name="export" code="R*chEXPT" description="Export an object">
      <direct-parameter type="specifier" optional="yes" description="object to export"/>
      <parameter name="to" code="kfil" type="alias" optional="yes" description="where to save the exported data"/>
    </command>
    <command name="apply text factory" code="R*chAply" description="Applies the operations in a specified text factory to the designated items">
      <direct-parameter type="file specification" description="the text factory document to apply"/>
      <parameter name="to" code="to  " type="any" description="where to apply the factory (defaults to the tell-target of the event)"/>
      <parameter name="options" code="Opts" type="Search Options" optional="yes" description="options for the application (only “returning results” and “showing results” make sense here)"/>
      <parameter name="filter" code="Filt" type="filter" optional="yes" description="file filtering criteria"/>
      <parameter name="saving" code="savo" type="savo" optional="yes" description="save each file after processing?"/>
      <parameter name="recursion" code="Recu" type="boolean" optional="yes" description="process nested folders?"/>
      <parameter name="text files only" code="TxtO" type="boolean" optional="yes" description="only process text files?"/>
      <parameter name="skip shielded folders" code="(..)" type="boolean" optional="yes" description="skip folders whose names are enclosed in (parentheses)?"/>
      <parameter name="search invisible folders" code="Invi" type="boolean" optional="yes" description="process invisible folders?"/>
      <parameter name="search compressed files" code="Cmp?" type="boolean" optional="yes" description="process compressed files?"/>
    </command>
    <command name="add line breaks" code="R*chIHLB" description="Inserts a hard line break character at the end of each soft-wrapped line">
      <direct-parameter type="specifier" description="text to be modified"/>
    </command>
    <command name="add prefix" code="R*chApfx" description="inserts a string at the beginning of each line in the indicated text">
      <direct-parameter type="specifier" description="text to be modified"/>
      <parameter name="prefix" code="pfxS" type="text" description="text to be prepended to each line"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="add suffix" code="R*chAsfx" description="inserts a string at the end of each line in the indicated text">
      <direct-parameter type="specifier" description="text to be modified"/>
      <parameter name="suffix" code="sfxS" type="text" description="text to be appended to each line"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="remove prefix" code="R*chRpfx" description="removes a string from the beginning of each line in the indicated text">
      <direct-parameter type="specifier" description="text to be modified"/>
      <parameter name="prefix" code="pfxS" type="text" description="text to be removed from the start of to each line"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="remove suffix" code="R*chRsfx" description="removes a string from the end of each line in the indicated text">
      <direct-parameter type="specifier" description="text to be modified"/>
      <parameter name="suffix" code="sfxS" type="text" description="text to be removed from the end of each line"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="add prefix and suffix" code="R*chApsx" description="simultaneously adds a prefix and suffix to each line in the indicated text">
      <direct-parameter type="specifier" description="text to be modified"/>
      <parameter name="prefix" code="pfxS" type="text" description="text to be prepended to each line"/>
      <parameter name="suffix" code="sfxS" type="text" description="text to be appended to each line"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="remove prefix and suffix" code="R*chRpsx" description="simultaneously removes a prefix and suffix from each line in the indicated text">
      <direct-parameter type="specifier" description="text to be modified"/>
      <parameter name="prefix" code="pfxS" type="text" description="text to be removed from the start of each line"/>
      <parameter name="suffix" code="sfxS" type="text" description="text to be removed from the end of each line"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="add line numbers" code="R*chAdLn" description="numbers each line in the indicated text">
      <direct-parameter type="specifier" description="text to be modified"/>
      <parameter name="start number" code="StNm" type="integer" optional="yes" description="the first line number (defaults to 1)"/>
      <parameter name="increment" code="NmIn" type="integer" optional="yes" description="add this value to each line number (defaults to 1)"/>
      <parameter name="adding space" code="AdSp" type="boolean" optional="yes" description="add a space after each line number? (defaults to true)"/>
      <parameter name="keeping justified" code="Jstf" type="boolean" optional="yes" description="right-justify the line numbers? (defaults to true)"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="remove line numbers" code="R*chRmLn" description="removes leading numbering from each line in the indicated text">
      <direct-parameter type="specifier" description="text to be modified"/>
      <parameter name="adding space" code="AdSp" type="boolean" optional="yes" description="remove any space after each line number? (defaults to true)"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="change case" code="R*chCase" description="Changes the case of text">
      <direct-parameter type="specifier" description="text to be changed"/>
      <parameter name="making" code="Cact" type="Cact" optional="yes" description="case transformation to apply"/>
    </command>
    <command name="clear undo history" code="R*chClrU" description="Deletes all Undo/Redo history information for this document">
      <direct-parameter type="specifier" optional="yes"/>
      <parameter name="confirming" code="Cnfm" type="boolean" optional="yes" description="ask the user to confirm the operation?"/>
    </command>
    <command name="compare" code="R*chFDif" description="compares two files or folders">
      <direct-parameter type="file specification" description="the new file/folder"/>
      <parameter name="against" code="OldI" type="file specification" description="the old file/folder"/>
      <parameter name="options" code="difO" type="Compare Options" optional="yes" description="comparison options"/>
      <parameter name="filter" code="cflO" type="filter" optional="yes" description="file filtering criteria (for multi-file compare)"/>
      <result type="Compare Results" optional="yes" description="information about the compare results"/>
    </command>
    <command name="convert to ASCII" code="R*ch2asc" description="Converts 8-bit characters to 7-bit characters">
      <direct-parameter type="specifier" description="text to be processed"/>
      <parameter name="replacing target" code="RSel" type="boolean" optional="yes" description="replace the text to be processed with the results?"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="decrement quote level" code="R*chRQuo" description="removes one level of Internet-style quoting from each line of the specified text">
      <direct-parameter type="specifier" description="text to be processed"/>
    </command>
    <command name="detab" code="R*chDetb" description="Replace tabs with runs of spaces.">
      <direct-parameter type="specifier" description="text to be changed"/>
      <parameter name="tab width" code="TabW" type="integer" optional="yes" description="number of spaces per tab"/>
    </command>
    <command name="entab" code="R*chEntb" description="Replace runs of spaces with tabs.">
      <direct-parameter type="specifier" description="text to be changed"/>
      <parameter name="tab width" code="TabW" type="integer" optional="yes" description="number of spaces per tab"/>
    </command>
    <command name="educate quotes" code="R*chEduQ" description="Convert straight quotes into curly (typographer’s) quotes">
      <direct-parameter type="specifier" description="text to be processed"/>
      <parameter name="replacing target" code="RSel" type="boolean" optional="yes" description="replace the text to be processed with the results?"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="straighten quotes" code="R*chDumQ" description="Convert curly (typographer’s) quotes into straight quotes">
      <direct-parameter type="specifier" description="text to be processed"/>
      <parameter name="replacing target" code="RSel" type="boolean" optional="yes" description="replace the text to be processed with the results?"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="find" code="R*chFind" description="Searches for text">
      <direct-parameter type="text" description="the text to search for"/>
      <parameter name="searching in" code="FnIn" type="any" optional="yes" description="where to search for the string (defaults to the tell-target of the event)"/>
      <parameter name="selecting match" code="SelM" type="boolean" optional="yes" description="select the search match if found?"/>
      <parameter name="options" code="Opts" type="Search Options" optional="yes" description="options for the search"/>
      <parameter name="excluding matches" code="Excl" type="boolean" optional="yes" description="for multi-file searches, list files that don’t match the search string?"/>
      <parameter name="filter" code="Filt" type="filter" optional="yes" description="file filtering criteria"/>
      <parameter name="recursion" code="Recu" type="boolean" optional="yes" description="search nested folders?"/>
      <parameter name="text files only" code="TxtO" type="boolean" optional="yes" description="only search text files?"/>
      <parameter name="skip shielded folders" code="(..)" type="boolean" optional="yes" description="skip folders whose names are enclosed in (parentheses)?"/>
      <parameter name="search invisible folders" code="Invi" type="boolean" optional="yes" description="search invisible folders?"/>
      <parameter name="search compressed files" code="Cmp?" type="boolean" optional="yes" description="process compressed files?"/>
      <result type="Search Match" optional="yes" description="information about the text string found by the search"/>
    </command>
    <command name="grep substitution" code="R*chGSub" description="computes the Grep replacement string based on the results of the most recent scripted Grep search">
      <parameter name="of" code="RplP" type="text" optional="yes" description="the Grep replacement pattern"/>
      <result type="text" description="the text of the substituted replacement pattern"/>
    </command>
    <command name="hard wrap" code="R*chWrap" description="hard-wraps text to conform to the parameters">
      <direct-parameter type="specifier" description="text to be hard-wrapped"/>
      <parameter name="limit" code="WLmt" type="WrMd" optional="yes" description="determines how the maximum line length is computed (defaults to “character width” if not supplied)"/>
      <parameter name="paragraph fill" code="PgFl" type="boolean" optional="yes" description="fill paragraphs to fill wrap width? (defaults to “false” if not supplied)"/>
      <parameter name="width" code="Wdth" type="integer" optional="yes" description="when wrapping to a character width, specifies maximum line length in characters (defaults to 72 if not supplied)"/>
      <parameter name="indentation" code="Indt" type="Indt" optional="yes" description="indentation mode of wrapped lines (defaults to “none” if not supplied"/>
      <parameter name="relative" code="FrsR" type="boolean" optional="yes" description="indent relative to first line? (defaults to “false” if not supplied)"/>
    </command>
    <command name="increment quote level" code="R*chIQuo" description="adds one level of Internet-style quoting to each line of the specified text">
      <direct-parameter type="specifier" description="text to be processed"/>
      <parameter name="quote string" code="Qstr" type="text" optional="yes" description="the quote string; if omitted, BBEdit will use the Internet Config quote string"/>
    </command>
    <command name="mark" code="R*chF&amp;Mk" description="mark matches of a specified search pattern with a string">
      <direct-parameter type="specifier" description="text to be marked"/>
      <parameter name="options" code="FMKo" type="Mark Options" description="options for the search"/>
    </command>
    <command name="normalize line endings" code="R*chNLE " description="convert any CRLF pairs and LF characters remaining after line-break conversion">
      <direct-parameter type="specifier" description="text to be modified"/>
      <result type="text" description="the modified text"/>
    </command>
    <command name="process duplicate lines" code="R*chPDuL" description="analyze the specified text for duplicate lines">
      <direct-parameter type="specifier" description="text to be processed"/>
      <parameter name="duplicates options" code="PdDO" type="Process Duplicates Options" optional="yes"/>
      <parameter name="output options" code="SlOO" type="Process Duplicates Output Options" optional="yes"/>
      <result type="Process Duplicates Results"/>
    </command>
    <command name="process lines containing" code="R*chProC" description="search for lines in the specified text containing the indicated string">
      <direct-parameter type="specifier" description="text to be processed"/>
      <parameter name="matching string" code="CntS" type="text" description="the string or Grep pattern to search for"/>
      <parameter name="matching with grep" code="LCGp" type="boolean" optional="yes" description="treat the matching string as a Grep pattern?"/>
      <parameter name="case sensitive" code="Case" type="boolean" optional="yes" description="honor character case differences when matching?"/>
      <parameter name="output options" code="SlOO" type="Process Lines Containing Output Options" optional="yes"/>
      <result type="Process Lines Containing Results" description="record containing the matched lines and the text with the matched lines removed if appropriate"/>
    </command>
    <command name="remove line breaks" code="R*chRHLB" description="Removes hard line-break characters and line-leading white space from the specified text, making it suitable for soft-wrapping.">
      <direct-parameter type="specifier" description="text to be modified"/>
    </command>
    <command name="replace" code="R*chRepl" description="performs a Replace All">
      <direct-parameter type="text" optional="yes" description="the text to search for"/>
      <parameter name="using" code="Repl" type="text" optional="yes" description="the replacement string (or pattern, if using Grep)"/>
      <parameter name="searching in" code="FnIn" type="any" optional="yes" description="where to replace the string (defaults to the tell-target of the event)"/>
      <parameter name="searchingString" code="RplS" type="text" optional="yes" description="string in which to search and replace"/>
      <parameter name="options" code="Opts" type="Search Options" optional="yes" description="options for the search"/>
      <parameter name="saving" code="savo" type="savo" optional="yes" description="if a multi-file replace, save after doing the replace?"/>
      <parameter name="recursion" code="Recu" type="boolean" optional="yes" description="search nested folders?"/>
      <parameter name="text files only" code="TxtO" type="boolean" optional="yes" description="only search text files?"/>
